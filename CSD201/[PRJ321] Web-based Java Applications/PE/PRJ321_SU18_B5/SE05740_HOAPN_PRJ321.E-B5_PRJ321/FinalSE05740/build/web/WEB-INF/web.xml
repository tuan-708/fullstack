<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>SearchSe</servlet-name>
        <servlet-class>servlet.SearchSe</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EnterName</servlet-name>
        <servlet-class>servlet.EnterName</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReHome</servlet-name>
        <servlet-class>servlet.ReHome</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReSub</servlet-name>
        <servlet-class>servlet.ReSub</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReUpdate</servlet-name>
        <servlet-class>servlet.ReUpdate</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SearchSe</servlet-name>
        <url-pattern>/SearchSe</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EnterName</servlet-name>
        <url-pattern>/EnterName</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReHome</servlet-name>
        <url-pattern>/ReHome</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReSub</servlet-name>
        <url-pattern>/ReSub</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReUpdate</servlet-name>
        <url-pattern>/ReUpdate</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>Home.jsp</welcome-file>
    </welcome-file-list>
</web-app>
